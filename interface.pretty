}

@SerializeJson
@Pascal->Kotlin
class FormSelect: FormInput {
  optionCaptions: String[]
  optionValues: String[]
}

@SerializeJson
@Pascal->Kotlin
class FormParams: FastInterfacedObject {
  inputs: FormInput[]
}

