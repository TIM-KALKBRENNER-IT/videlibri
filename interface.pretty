
@SerializeJson
@Pascal->Kotlin
class FormInput {
  name: String
  caption: String
  value: String
}

@SerializeJson
@Pascal->Kotlin
class FormSelect: FormInput {
  optionCaptions: String[]
  optionValues: String[]
}

@SerializeJson
@Pascal->Kotlin
class FormParams: FastInterfacedObject {
  inputs: FormInput[]
}

@Pascal->Kotlin
class VersionInfo {
  version: String
  platform: String
}

@SerializeJson
@KotlinVar
@KotlinDataClass
@Pascal<->Kotlin
class BookListDisplayOptions {
  showHistory: Boolean
  noBorrowedBookDetails: Boolean
  showRenewCount: Boolean = true
  groupingKey: String = "_dueWeek"
  sortingKey: String = "dueDate"
  filterKey: String
}



fun VLGetVersion(): VersionInfo

